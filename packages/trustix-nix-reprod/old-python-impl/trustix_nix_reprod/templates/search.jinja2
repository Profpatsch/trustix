{% extends "includes/base.jinja2" %}

{% block main %}

  {% for attr, derivations in model.derivations_by_attr.items() %}
    <h3>{{attr}}</h3>

    <ul>
      {% for derivation in derivations %}
        <li>
          <a href="{{url_reverse("drv", drv_path=drv_url_quote(derivation))}}">
            {{derivation}}
          </a>
        </li>
      {% endfor %}
    </ul>

  {% endfor %}

{% endblock %}
